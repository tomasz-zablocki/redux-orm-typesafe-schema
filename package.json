{
  "name": "redux-orm-typesafe-schema",
  "description": "A small static-typing oriented wrapper around redux-orm written in TypeScript",
  "version": "0.0.11",
  "author": {
    "name": "Tomasz ZabÅ‚ocki",
    "email": "tomasz.zablocki@gmail.com"
  },
  "keywords": [
    "redux",
    "redux-orm",
    "typescript",
    "typesafe",
    "static-typing"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "license": "MIT",
  "private": false,
  "scripts": {
    "tsc": "tsc -p ./tsconfig.build.json",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.config.js --no-cache",
    "test:ci": "jest --config jest.config.js --collectCoverage --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --collectCoverage",
    "preversion": "yarn test",
    "postversion": "git push --tags && yarn publish . && git push",
    "prepublish": "yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomasz-zablocki/redux-orm-typesafe-schema"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=11.10.1",
    "yarn": ">=1.15.2"
  },
  "dependencies": {
    "redux-orm": "^0.13.3",
    "typesafe-actions": "^4.2.1",
    "utility-types": "^3.7.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.2",
    "@types/redux-logger": "^3.0.7",
    "dts-jest": "^23.0.0",
    "husky": "^2.3.0",
    "jest": "^24.7.1",
    "jest-junit": "^6.4.0",
    "jest-watch-suspend": "^1.1.2",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "latest",
    "redux-logger": "latest",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
